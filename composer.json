{
    "name": "medicalmundi/brotea",
    "description": "A library for OpenEMR modules",
    "type": "library",
    "keywords": ["openemr", "openemr-module"],
    "require": {
        "PHP": "^7.2 | ^8.0",
        "psr/container": "^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "target-directory": "tools"
        }
    },
    "autoload": {
        "psr-4": {
            "Brotea\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotea\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.16",
        "bamarni/composer-bin-plugin": "^1.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zerai Teclai",
            "email": "teclaizerai@gmail.com"
        }
    ]
}
